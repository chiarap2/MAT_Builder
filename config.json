{
      "preprocessing1": [
        {
          "Input": {
            "id": "path",
            "type": "text",
            "placeholder": "path"
          }
        },
        {
          "Input": {
            "id": "kmh",
            "type": "number",
            "placeholder": "km/h"
          }
        },
        {
          "Input": {
            "id": "n_point",
            "type": "number",
            "placeholder": "n point"
          }
        }
      ],
      "preprocessing2": [
        {
          "Input": {
            "id": "path",
            "type": "text",
            "placeholder": "path"
          }
        },
        {
          "Input": {
            "id": "kmh",
            "type": "number",
            "placeholder": "km/h"
          }
        }
      ],
      "stops_and_moves": [
        {
          "Input": {
            "id": "minutes",
            "type": "number",
            "placeholder": "Minutes"
          }
        },
        {
          "Input": {
            "id": "radius",
            "type": "number",
            "placeholder": "Search radius"
          }
        }
      ],
      "stop_move_enrichment": [
        {
          "H5": {
            "children": "Move enrichment: "
          }
        },
        {
          "Dropdown": {
            "id": "enrich_move",
            "options": [{"label": "yes", "value": "yes"},{"label":"no","value":"no"}]
          }
        },
        {
          "H5": {
            "children": "Stop enrichment: "
          }
        },
        {
          "Span": {
            "children": "Insert the name of the city (to download PoIs from OpenStreetMap): "
          }
        },
        {
          "Input": {
            "id": "place",
            "type": "text",
            "placeholder": "City"
          }
        },
        {
          "Dropdown": {
            "id": "list_poi",
            "options": [{"label": "amenity", "value": "amenity"},
              {"label": "aeroway", "value": "aeroway"},
              {"label": "building", "value": "building"},
              {"label": "historic", "value": "historic"},
              {"label": "healthcare", "value": "healthcare"},
              {"label": "landuse", "value": "landuse"},
              {"label": "office", "value": "office"},
              {"label": "public_transport", "value": "public_transport"},
              {"label": "shop", "value": "shop"},
              {"label": "tourism", "value": "tourism"},
              {"label":"no enrichment","value":"no"}]
          }
        },
        {
          "Span": {
            "children": "... or upload your file (write 'no' if no file is uploaded): "
          }
        },
        {
          "Input": {
            "id": "place",
            "type": "text",
            "placeholder": "path/to/your/file"
          }
        },
        {
          "Span": {
            "children": "Choose semantic granularity (%): "
          }
        },
        {
          "Input": {
            "id": "granularity",
            "type": "number",
            "placeholder": "Semantic Granularity"
          }
        },
        {
          "Span": {
            "children": "Max distance from stops: "
          }
        },
        {
          "Input": {
            "id": "max_distance",
            "type": "number",
            "placeholder": "Max distance from PoIs"
          }
        },
        {
          "H5": {
            "children": "User enrichment: "
          }
        },
        {
          "Dropdown": {
            "id": "user_enrich",
            "options": [{"label": "Social media posts", "value": "post"},
                        {"label":"no enrichment","value":"no"}]
          }
        },
        {
          "Span": {
            "children": "... or upload your file (write 'no' if no file is uploaded): "
          }
        },
        {
          "Input": {
            "id": "place",
            "type": "text",
            "placeholder": "path/to/your/file"
          }
        },
        {
          "H5": {
            "children": "Trajectory enrichment: "
          }
        },
        {
          "Dropdown": {
            "id": "traj_enrich",
            "options": [{"label": "Social media posts", "value": "post"},
                        {"label":"no enrichment","value":"no"}]
          }
        },
        {
          "Span": {
            "children": "... or upload your file (write 'no' if no file is uploaded): "
          }
        },
        {
          "Input": {
            "id": "place",
            "type": "text",
            "placeholder": "path/to/your/file"
          }
        }
      ]
}
